                        <div class="form-group">
                            <label class="control-label col-sm-4" style="text-align:left;" for="desc">ASSET DESC:</label>
                            <div class="col-sm-8">
                                <input type="text" class="form-control" style="width:160%" id="desc">
                            </div>
                        </div>
